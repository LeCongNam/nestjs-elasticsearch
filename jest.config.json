{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "modulePaths": [
        "<rootDir>",
        "src"
    ],
    "rootDir": "./src",
    "testRegex": ".spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "restoreMocks": true,
    "resetMocks": true
}